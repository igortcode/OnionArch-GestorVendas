@using Gestao.Application.DTO.Generic;
@model GList<Gestao.Application.DTO.Comanda.ObterComandaDTO>

@{
    ViewData["Title"] = "Comandas";
}

@await Html.PartialAsync("../Shared/_Notification")

<div class="d-none d-sm-block">
    @await Html.PartialAsync("../Comanda/_ListDesktopPartial", Model)
</div>

<div class="d-block d-sm-none">
    @await Html.PartialAsync("../Comanda/_ListMobilePartial", Model)
</div>


<input type="hidden" id="aberturaDia" value="@ViewBag.IdAberturaDia" />


